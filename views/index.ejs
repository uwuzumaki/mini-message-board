<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <ul>
      <% messages.forEach((message, index) => { %>
      <h3><%= message.user %></h3>
      <h2><%= message.text %></h2>
      <h4><%= message.added %></h4>
      <a href="/messages/<%= index %>">Open</a>
      <% }) %>
    </ul>
    <a href="/new">Write a message!</a>
  </body>
</html>
